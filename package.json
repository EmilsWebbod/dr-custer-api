    "@types/qs": "^6.5.1",
    "@types/storybook__react": "^3.0.9",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "styled-components": "^3.4.5",
    "uuid": "^3.3.2"
